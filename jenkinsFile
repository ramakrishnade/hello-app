@Library("Jenkins-pipeline-library") _

pipeline {
    agent any
    stages {
        stage('Code Checkout') {
            steps {
               script {
                  sayHello()
                    echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
                    echo "BUILD Number $(env.BUILD_NUMBER) and BUILD_TAG $(env.BUILD_TAG)"
                }
            }
        }
    }
}
